/* pages/user/apply/apply.wxss */
view.container {
  display: flex;
  width: 100%;
  height: 100%;
  padding-top: 10%;

}
text.head {
  padding-top: 0;
  margin-bottom: 12%;
}
scroll-view {
  width: 80%;
  height: 100%;
}
view.item {
  width: 95%;
  background-color: rgb(87, 233, 172);
  margin-bottom: 20rpx;
  box-shadow: 3rpx 3rpx 3rpx 3rpx #888;
}
view.sub {
  display: inline-flex;
  margin-bottom: 5rpx;
  width:100%;
}
.pre {
  width: 30%;
  margin-right: 4%;
  text-align: right;
}
.fix {
  width: 66%;
}
view.sub button {
  width: 30%;
  margin-left: 0;
  padding: 0;
  border-radius: 0;
}