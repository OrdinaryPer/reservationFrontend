/* pages/user/profile/update/update.wxss */
view.container {
  display: flex;
  width: 100%;
  height: 100%;
  padding-top: 25%;
  margin: 0;
}
text.head {
  margin-bottom: 10%;
}
form {
  display: flex;
  justify-content: center;
  padding-top: 5%;
  width: 90%;
  height: 60%;
  padding-bottom: 35%;
  margin: 0;

}
view.integ {
  width: 100;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background-color: rgb(87, 233, 172);
  box-shadow: 3rpx 3rpx 3rpx 3rpx #666;
}
view.item {
  display: inline-flex;
  align-items: center;
  width: 100%;
  margin: 0;
  padding: 0;
  margin-bottom: 20rpx;
  border-radius: 3rpx;
}
text.pre {
  width:35%;
  text-align: right;
  margin-right: 3%;
}
input {
  width: 60%;
  background-color: #fff;
  border-radius: 10rpx;
}
form button {
  padding: 0;
  margin-top: 5%;
  width: 30%;
}



